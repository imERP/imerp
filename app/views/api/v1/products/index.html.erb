<p id="notice"><%= notice %></p>

<h1>Api V1 Products</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Standard</th>
      <th>Pattern</th>
      <th>Cost</th>
      <th>Price</th>
      <th>Use</th>
      <th>Explanation</th>
      <th>Can sale</th>
      <th>Sales</th>
      <th>Sales amount</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_products.each do |api_v1_product| %>
      <tr>
        <td><%= api_v1_product.code %></td>
        <td><%= api_v1_product.name %></td>
        <td><%= api_v1_product.standard %></td>
        <td><%= api_v1_product.pattern %></td>
        <td><%= api_v1_product.cost %></td>
        <td><%= api_v1_product.price %></td>
        <td><%= api_v1_product.use %></td>
        <td><%= api_v1_product.explanation %></td>
        <td><%= api_v1_product.can_sale %></td>
        <td><%= api_v1_product.sales %></td>
        <td><%= api_v1_product.sales_amount %></td>
        <td><%= api_v1_product.user_id %></td>
        <td><%= link_to 'Show', api_v1_product %></td>
        <td><%= link_to 'Edit', edit_api_v1_product_path(api_v1_product) %></td>
        <td><%= link_to 'Destroy', api_v1_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Product', new_api_v1_product_path %>
